@using TimeTableGenerator.ViewModel
@model TimeTableViewModel

<h2>Generated Time Table</h2>

<table class="table table-bordered">
    @for (int i = 0; i < Model.InitialInput.SubjectsPerDay; i++)
    {
        <tr>
            @for (int j = 0; j < Model.InitialInput.WorkingDays; j++)
            {
                <td>@Model.GeneratedTable[i, j]</td>
            }
        </tr>
    }
</table>